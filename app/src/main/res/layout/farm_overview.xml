<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/farmLineChartLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <com.github.mikephil.charting.charts.CombinedChart
            android:id="@+id/farmCombineChart"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:clipChildren="false"
            app:layout_constraintEnd_toEndOf="@id/farmLineChartLayout"
            app:layout_constraintStart_toStartOf="@id/farmLineChartLayout"
            app:layout_constraintTop_toTopOf="@id/farmLineChartLayout"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginTop="20dp">

    </com.github.mikephil.charting.charts.CombinedChart>

</androidx.constraintlayout.widget.ConstraintLayout>
